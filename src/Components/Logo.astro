---
import Image from "astro/components/Image.astro"
import img from "../assets/logo_naranja.png";

const { classList } = Astro.props;
---
<div id="logo" class:list={["w-26 h-6 sm:w-38 sm:h-9 xl:w-44 xl:h-11",classList]}>
    <a href="/">
        <Image src={img} alt={"Logo de Hulux Telecomunicaciones"}></Image>
    </a>
</div>